{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { stuffService } from '@/_services';\nexport default {\n  name: 'stuffFetch',\n  data() {\n    return {\n      things: []\n    };\n  },\n  methods: {\n    watchId() {\n      navigator.geolocation.watchPosition(position => {\n        const {\n          latitude,\n          longitude\n        } = position.coords;\n        document.write(this.latitude, this.longitude);\n      });\n    },\n    refresh() {\n      stuffService.getAllstuff().then(res => {\n        console.log(res.data);\n        this.things = res.data;\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    goEdit(uid) {\n      console.log(uid);\n      // this.$router.push( '/updatething/'+id)\n      this.$router.push({\n        name: 'updatething',\n        params: {\n          id: uid\n        }\n      });\n    }\n  },\n  //recuperation de toutes les chose et pacer dans le data\n  mounted() {\n    stuffService.getAllstuff().then(res => {\n      stuffService.watchId();\n      console.log(res.data);\n      this.things = res.data;\n    }).catch(err => {\n      console.log(err);\n    });\n  },\n  Updated() {\n    stuffService.getAllstuff().then(res => {\n      console.log(res.data);\n      this.things = res.data;\n    }).catch(err => {\n      console.log(err);\n    });\n  }\n};","map":{"version":3,"mappings":";AAgDE,SAAQA,YAAY,QAAO,aAAY;AAGvC,eAAe;EAEfC,IAAI,EAAE,YAAY;EAElBC,IAAI,GAAE;IAEJ,OAAO;MAGEC,MAAM,EAAC;IAEhB;EACF,CAAC;EAIAC,OAAO,EAAC;IAGXC,OAAO,GAAE;MAGPC,SAAS,CAACC,WAAW,CAACC,aAAa,CAACC,QAAO,IAAK;QAChD,MAAM;UAAEC,QAAQ;UAAEC;QAAU,IAAIF,QAAQ,CAACG,MAAM;QAC/CC,QAAQ,CAACC,KAAK,CAAE,IAAI,CAACJ,QAAQ,EAAC,IAAI,CAACC,SAAS,CAAC;MAC/C,CAAC;IACD,CAAC;IAEDI,OAAO,GAAE;MAETf,YAAY,CAACgB,WAAW,EAAC,CACQC,IAAI,CAACC,GAAG,IAAE;QAETC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAChB,IAAI;QAEpB,IAAI,CAACC,MAAM,GAACe,GAAG,CAAChB,IAAG;MAGrB,CAAC,EACEmB,KAAK,CAACC,GAAG,IAAE;QAACH,OAAO,CAACC,GAAG,CAACE,GAAG,CAAC;MAAA,CAAC;IAIhE,CAAC;IAGDC,MAAM,CAA